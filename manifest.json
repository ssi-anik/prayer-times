{
    "name": "Prayer times",
    "description": "Prayer times is a chrome extension helps your find the epochs and salat time for your timezone.",
    "manifest_version": 2,
    "version": "1.0",
    "permissions": [
        "webRequest",
        "geolocation"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon128.png",
        "default_title": "Click here!",
        "default_popup": "index.html"
    }
}